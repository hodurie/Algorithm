SELECT o.ANIMAL_ID AS ANIMAL_ID, o.ANIMAL_TYPE AS ANIMAL_TYPE, o.NAME AS NAME
FROM ANIMAL_INS AS i
RIGHT JOIN ANIMAL_OUTS AS o
USING (ANIMAL_ID)
WHERE (SEX_UPON_INTAKE LIKE ('I%')
       AND SEX_UPON_INTAKE IS NOT NULL)
       AND (SEX_UPON_OUTCOME LIKE ('S%')
            OR SEX_UPON_OUTCOME LIKE ('N%'));